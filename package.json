{
  "name": "databaker",
  "version": "0.1.0",
  "description": "Data analyze and Plot on Node.js",
  "keywords": [
    "data",
    "csv",
    "json",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/databaker.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/**/*.ts",
    "test:unit": "jest test"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "[^/]*\\.test.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "canvas": "^2.5.0",
    "core-js": "^3.0.0",
    "csvtojson": "^2.0.8",
    "d3-color": "^1.2.3",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "json5": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@types/d3": "5.7.2",
    "@types/jest": "26.0.4",
    "@types/node": "11.13.11",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "eslint": "5.16.0",
    "jest": "24.8.0",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  }
}
